<!DOCTYPE html>
<html>
	<head>
        {{>head}}
		<title>{{page_title}}</title>
		<style type="text/css"></style>
	</head>
	<body>
		{{>header}}
        <article id="menu">

            <h2>{{today_menu}}</h2>
			<ul class="formule today">
    			<li class="menu-item"><a href="#today">{{today_description}}</a></li>
			</ul>
			<hr>
    		<h2>{{discover_menu}}</h2>
    		<ul class="formule discover">
    			{{#each discover_menu_items}}
                <li class="menu-item">
					<a href="#{{this.identifier}}">
						<label>{{this.label}}</label>
						<span class="dotted-line"></span>
						<span class="duration">{{this.duration}}</span>
					</a>
				</li>
                {{/each}}
    		</ul>
			<hr>
    		<h2>{{alacarte_menu}}</h2>
			<ul class="formule carte">
    			{{#each alacarte_menu_items}}
				<li class="menu-item">
					<a href="#{{this.identifier}}">
						<label>{{this.label}}</label>
						<span class="dotted-line"></span>
						<span class="duration">{{this.duration}}</span>
					</a>
				</li>
                {{/each}}
    		</ul>
			<hr>
    	</article>

		{{>footer}}
	</body>
	{{!-- <script data-main="/assets/js/main.js" src="/assets/vendor/requirejs/require.js"></script> --}}
</html>
